set autocalc
set numeric
set numeric_decimal=0
set overlap
set xlsx_readformulas
set half_page_scroll

nnoremap "ZZ" ":wq<cr>"
nnoremap "ZQ" ":q!<cr>"
nnoremap "<C-D>" "<C-F>"
nnoremap "<C-U>" "<C-B>"
nnoremap " p" ":cpaste<cr>"
nnoremap " y" ":ccopy<cr>"
nnoremap " c" ":int recalc<cr>"
nnoremap " O" ":set overlap<cr>"
nnoremap " o" ":set nooverlap<cr>"
